{
	"port": "8080",

	"rsi": {
		"strip": 8,
		"cache": {
			"root": "./cache",
			"dirlen": 2
		},
		"temp-prefix": "rsi-",
		"link-regex": "(?:(?:href|src)=|(?:url|import)\\()[\"']?([^\"'\\)]+)(?:[\"']?|\\)|>| )",
		"ignore-extensions": [".html", ".htm"]
	},

	"redis": {
		"host": "redis.dev",
		"port": 6379,
		"db": 1
	}
}